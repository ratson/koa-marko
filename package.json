{
  "name": "koa-marko",
  "version": "0.1.0",
  "description": "Marko template engine middleware for koa",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "preexample": "npm run build",
    "example": "babel-node example/app",
    "clean": "rimraf lib dist coverage",
    "lint": "csc",
    "prepublish": "npm-run-all clean build",
    "test": "npm run lint"
  },
  "peerDependencies": {
    "marko": "*"
  },
  "optionalDependencies": {
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1",
    "cross-env": "^4.0.0",
    "csc": "^0.6.1",
    "koa": "^2.2.0",
    "marko": "^4.2.8",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1"
  },
  "repository": "ratson/koa-marko",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ]
}
