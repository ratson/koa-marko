{
  "name": "koa-marko",
  "version": "0.0.1",
  "description": "Marko template engine middleware for koa",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "preexample": "npm run build",
    "example": "babel-node example/app",
    "clean": "rimraf lib dist coverage",
    "lint": "csc",
    "prepublish": "npm-run-all clean build",
    "test": "npm run lint"
  },
  "dependencies": {
    "marko": "^3.12.1"
  },
  "optionalDependencies": {
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-env": "^0.0.8",
    "babel-register": "^6.18.0",
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1",
    "cross-env": "^3.1.3",
    "csc": "^0.3.0",
    "koa": "^2.0.0-alpha.7",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4"
  },
  "repository": "ratson/koa-marko",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ]
}
